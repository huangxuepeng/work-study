                },
                "dataName": {
                    "type": "string"
                },
                "logType": {
                    "type": "integer"
                },
                "operatorTime": {
                    "type": "integer"
                },
                "srcType": {
                    "type": "integer"
                },
                "uid": {
                    "type": "integer"
                },
                "userId": {
                    "type": "string"
                }
            }
        }
    }
